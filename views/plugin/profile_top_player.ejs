
  <% for(var i=0; i < data.length; i++) { %>

    <div class="col-sm-2 mt-3 m-1 bg-light shadow rounded" style="width: 160px;min-width: 160px;">
        <div class="text-center card-box">
          <div class="member-card pt-2 pb-2">
            <div class="thumb-lg member-thumb mx-auto"><img class="rounded-circle img-thumbnail" src="https://bootdey.com/img/Content/avatar/avatar2.png" style="width: 5rem;" alt="profile-image"></div>
            <div class="mt-3">
              <h6><%= data[i].name %> </h6>
              <p class="text-muted">Specialty<span> | </span><span><a class="text-pink" href="#"><%= data[i].specialty %></a></span></p>
            </div>
            <!-- <button class="btn btn-primary btn-rounded waves-effect w-md waves-light" type="button">Match Detail</button> -->
          </div>
        </div>
      </div>
    <% } %>